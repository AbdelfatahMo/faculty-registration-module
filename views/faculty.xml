<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="faculty_view_form" model="ir.ui.view">
            <field name="name">skill.view.form</field>
            <field name="model">registration.faculty</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>

                            <field name="name" />

                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="faculty_view_tree" model="ir.ui.view">
            <field name="name">skill.view.tree</field>
            <field name="model">registration.faculty</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                </tree>
            </field>
        </record>


        <record id="faculty_action" model="ir.actions.act_window">
            <field name="name">Faculty</field>
            <field name="res_model">registration.faculty</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_noncontent_smiling_face">
                    Enter Faculties For students!
                </p>
        </field>
        </record>


    </data>


</odoo>